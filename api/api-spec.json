                                            "links": {
                                                "links": {
                                                    "first": "http://thepos.test/api/v1/products?page=1",
                                                    "last": "http://thepos.test/api/v1/products?page=1"
                                                }
                                            },
                                            "meta": {
                                                "current_page": 1,
                                                "from": 1,
                                                "last_page": 1,
                                                "path": "http://thepos.test/api/v1/products",
                                                "per_page": 15,
                                                "to": 5,
                                                "total": 5
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "$ref": "#/components/responses/Error"
                    }
                },
                "parameters": [
                    {
                        "schema": {
                            "type": "integer"
                        },
                        "in": "query",
                        "name": "page",
                        "description": "page number"
                    },
                    {
                        "schema": {
                            "type": "number"
                        },
                        "in": "query",
                        "name": "group",
                        "description": "fetch only products of that category"
                    }
                ]
