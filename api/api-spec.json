                                                {
                                                    "sku": "72527273070",
                                                    "name": "UPCA prodcut",
                                                    "price": 100,
                                                    "tax_rate": 10,
                                                    "taxed_price": 110,
                                                    "image": "https://pixnio.com/media/perfume-fancy-product-package-wedding-ring",
